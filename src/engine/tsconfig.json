{
   "compilerOptions": {
      "outFile": "../../build/dist/excalibur.amd.js",
      "sourceMap": true,
      "inlineSources": true,
      "experimentalDecorators": true,
      "target": "es5",
      "module": "amd",
      "declaration": true,
      "noImplicitAny": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true
   }
}